<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_initail"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.nakoyagarden.ekapop.restaurant.InitailActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/lbIaHost"
        android:ems="7"
        android:paddingLeft="@dimen/default_gap"
        android:paddingTop="@dimen/default_gap"
        android:layout_marginTop="@dimen/activity_vertical_margin"/>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtIaHost"
        android:ems="7"
        android:layout_alignBaseline="@+id/lbIaHost"
        android:layout_toRightOf="@+id/lbIaHost"/>
    <CheckBox
        android:text="CheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/chkPrintOrder"
        android:layout_alignBaseline="@+id/lbIaHost"
        android:layout_toRightOf="@+id/txtIaHost"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:layout_below="@+id/lbIaHost"
        android:id="@+id/lbIaPrint"
        android:ems="7"
        android:paddingLeft="@dimen/default_gap"
        android:paddingTop="@dimen/default_gap"
        android:layout_marginTop="@dimen/activity_vertical_margin"/>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtIaHost"
        android:id="@+id/txtIaPrint"
        android:ems="7"
        android:layout_alignBaseline="@+id/lbIaPrint"
        android:layout_toRightOf="@+id/lbIaPrint"/>
    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/cboIaPrinter"
        android:layout_alignBaseline="@+id/lbIaPrint"
        android:layout_toRightOf="@+id/txtIaPrint"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:layout_below="@+id/lbIaPrint"
        android:id="@+id/lbIaPosID"
        android:ems="7"
        android:paddingLeft="@dimen/default_gap"
        android:paddingTop="@dimen/default_gap"
        android:layout_marginTop="@dimen/activity_vertical_margin"/>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtIaPrint"
        android:id="@+id/txtIaPosID"
        android:inputType="numberSigned"
        android:ems="7"
        android:layout_alignBaseline="@+id/lbIaPosID"
        android:layout_toRightOf="@+id/lbIaPosID"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:layout_below="@+id/lbIaPosID"
        android:id="@+id/lbIaTaxID"
        android:ems="7"
        android:paddingLeft="@dimen/default_gap"
        android:paddingTop="@dimen/default_gap"
        android:layout_marginTop="@dimen/activity_vertical_margin"/>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtIaPosID"
        android:id="@+id/txtIaTaxID"
        android:ems="7"
        android:layout_alignBaseline="@+id/lbIaTaxID"
        android:layout_toRightOf="@+id/lbIaTaxID"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:layout_below="@+id/lbIaTaxID"
        android:id="@+id/lbIaPortID"
        android:ems="7"
        android:paddingLeft="@dimen/default_gap"
        android:paddingTop="@dimen/default_gap"
        android:layout_marginTop="@dimen/activity_vertical_margin"/>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtIaPortID"
        android:inputType="numberSigned"
        android:ems="7"
        android:layout_alignBaseline="@+id/lbIaPortID"
        android:layout_toRightOf="@+id/lbIaPortID" />
    <CheckBox
        android:text="CheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/chkPrintBill"
        android:layout_alignBaseline="@+id/lbIaPortID"
        android:layout_toRightOf="@+id/txtIaPortID"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:layout_below="@+id/lbIaPortID"
        android:id="@+id/lbIaWebDirectory"
        android:ems="7"
        android:paddingLeft="@dimen/default_gap"
        android:paddingTop="@dimen/default_gap"
        android:layout_marginTop="@dimen/activity_vertical_margin"/>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/lbIaWebDirectory"
        android:layout_alignBaseline="@+id/lbIaWebDirectory"
        android:id="@+id/txtIaWebDirectory"
        android:ems="7" />
    <CheckBox
        android:text="CheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/chkPrintCloseDay"
        android:layout_alignBaseline="@+id/lbIaWebDirectory"
        android:layout_toRightOf="@+id/txtIaWebDirectory"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/lbIaUserDB"
        android:layout_below="@+id/lbIaWebDirectory"
        android:ems="7"
        android:paddingLeft="@dimen/default_gap"
        android:paddingTop="@dimen/default_gap"
        android:layout_marginTop="@dimen/activity_vertical_margin"/>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtIaPosID"
        android:id="@+id/txtIaUserDB"
        android:ems="10"
        android:layout_alignBaseline="@+id/lbIaUserDB"
        android:layout_toRightOf="@+id/lbIaUserDB"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/lbIaPasswordDB"
        android:layout_below="@+id/lbIaUserDB"
        android:ems="7"
        android:paddingLeft="@dimen/default_gap"
        android:paddingTop="@dimen/default_gap"
        android:layout_marginTop="@dimen/activity_vertical_margin"/>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtIaPasswordDB"
        android:ems="7"
        android:layout_alignBaseline="@+id/lbIaPasswordDB"
        android:layout_toRightOf="@+id/lbIaPasswordDB"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:layout_below="@+id/lbIaPasswordDB"
        android:id="@+id/btnIaPrint"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:layout_marginLeft="@dimen/activity_vertical_margin"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/btnIaPrint"
        android:layout_alignBaseline="@+id/btnIaPrint"
        android:text="New Button"
        android:id="@+id/btnIaSave"
        android:layout_marginLeft="@dimen/activity_vertical_margin"/>
    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnIaTest"
        android:layout_alignBaseline="@+id/btnIaPrint"
        android:layout_toRightOf="@+id/btnIaSave"
        android:layout_marginLeft="@dimen/activity_vertical_margin"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:layout_below="@+id/btnIaPrint"
        android:id="@+id/btnFoodsView"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="@dimen/activity_vertical_margin"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:layout_below="@+id/btnFoodsView"
        android:id="@+id/btnMFoodsType"
        android:layout_marginLeft="@dimen/activity_vertical_margin"/>
    <!--<Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:layout_below="@+id/btnMFoodsType"
        android:id="@+id/btnMTable"/>-->
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/btnMTable"
        android:layout_below="@+id/btnMFoodsType"
        android:layout_marginLeft="@dimen/activity_vertical_margin"
        android:layout_marginBottom="@dimen/activity_vertical_margin"/>
    <!--<Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/btnMArea"
        android:layout_below="@+id/btnMTable"/>-->
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/btnMArea"
        android:layout_alignBaseline="@+id/btnMTable"
        android:layout_toRightOf="@+id/btnMTable"
        android:layout_below="@+id/btnMFoodsType"
        android:layout_marginLeft="@dimen/activity_vertical_margin"/>
    <!--<Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/btnMRes"
        android:layout_marginBottom ="10dp"
        android:layout_below="@+id/btnMArea"/>-->
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/btnMRes"
        android:layout_alignBaseline="@+id/btnMTable"
        android:layout_toRightOf="@+id/btnMArea"
        android:layout_below="@+id/btnMFoodsType"
        android:layout_marginLeft="@dimen/activity_vertical_margin"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/btnMUser"
        android:layout_marginBottom ="10dp"
        android:layout_below="@+id/btnMRes"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:layout_marginLeft="@dimen/activity_vertical_margin"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/btnMBillVoid"
        android:layout_marginBottom ="10dp"
        android:layout_below="@+id/btnMUser"
        android:layout_marginLeft="@dimen/activity_vertical_margin"/>

</RelativeLayout>
